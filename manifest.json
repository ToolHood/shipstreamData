{
    "manifest_version": 3,
    "name": "Zendesk Data Extractor",
    "version": "1.1",
    "description": "1.1: Name kopieren",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://*.zendesk.com/*"
    ]
  }
  